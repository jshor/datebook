{
  "name": "datebook",
  "version": "1.0.2",
  "description": "Generates URLs and downloads ICS files for adding events to popular calendar apps.",
  "scripts": {
    "build": "parcel build ./src/index.js",
    "test": "jest",
    "docs": "esdoc",
    "test:ci": "jest && codecov",
    "commit": "yarn git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "standard-version -n"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jshor/datebook"
  },
  "keywords": [
    "icalendar",
    "google calendar",
    "ics",
    "yahoo calendar",
    "outlook",
    "calendar",
    "event"
  ],
  "author": "Josh Shor <jpshor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jshor/datebook/issues"
  },
  "homepage": "https://github.com/jshor/datebook",
  "dependencies": {
    "angular-addtocalendar": "^1.3.5",
    "file-saver": "^2.0.2",
    "moment": "^2.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "autoprefixer": "^6.0.3",
    "babel-jest": "^24.4.0",
    "babel-loader": "^8.0.5",
    "codecov": "^3.5.0",
    "commitizen": "^3.1.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "husky": "^2.4.0",
    "jest": "^24.1.0",
    "jest-mock-random": "^1.0.2",
    "parcel-bundler": "^1.12.3",
    "standard-version": "^6.0.1"
  },
  "gitHooks": {
    "pre-push": "yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
